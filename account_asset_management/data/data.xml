<odoo>
    <data>

        <!-- default group -->
        <record id="res_groups_machine_management_data" model="res.groups">
            <field name="name">Equipment Management</field>
            <field name="comment">Equipment Management Access rights</field>
            <field name="averigo_group_check" eval="True"/>
            <field name="default_groups" eval="True"/>
            <field name="menu_access"
                   eval="[(6, 0, [ref('account_asset_management.equipment_management_menu'), ref('account_asset_management.machine_management_menu'),
               ref('account_asset_management.machine_bill_menu'), ref('account_asset_management.menu_account_asset_transfer'),
               ref('account_asset_management.service_configuration_menu'), ref('account_asset_management.machine_type_menu'),
               ref('account_asset_management.machine_location_menu'), ref('account_asset_management.menu_asset_transfer_type'),
               ref('account_asset_management.menu_account_asset_profile'),])]"/>
            <field name="operator_id" eval="True"/>
            <field name="model_access" model="ir.model.access" eval="[(5, 0, 0),
            (0, 0, {
                'name': 'Equipment Management',
                'model_id': ref('account_asset_management.model_account_asset'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
            (0, 0, {
                'name': 'Equipment Bill',
                'model_id': ref('account_asset_management.model_account_move'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
            (0, 0, {
                'name': 'Equipment Transfer',
                'model_id': ref('account_asset_management.model_account_asset_transfer'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
            (0, 0, {
                'name': 'Equipment Type',
                'model_id': ref('account_asset_management.model_account_asset_type'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
            (0, 0, {
                'name': 'Equipment Location',
                'model_id': ref('account_asset_management.model_account_asset_location'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
            (0, 0, {
                'name': 'Equipment Transfer Type',
                'model_id': ref('account_asset_management.model_asset_transfer_type'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
            (0, 0, {
                'name': 'Equipment Profile',
                'model_id': ref('account_asset_management.model_account_asset_profile'),
                'perm_write': False,
                'perm_create': False,
                'perm_unlink': False
            }),
        ]"/>
        </record>

        <!-- machine management menu -->
        <record id="account_asset_management.equipment_management_menu" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.machine_management_menu" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.machine_bill_menu" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.menu_account_asset_transfer" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.service_configuration_menu" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.machine_type_menu" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.machine_location_menu" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.menu_asset_transfer_type" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="account_asset_management.menu_account_asset_profile" model="ir.ui.menu">
            <field name="averigo_menu" eval="True"/>
        </record>

        <record id="asset_transfer_type_in_wl" model="asset.transfer.type">
            <field name="sequence_code">IN</field>
            <field name="code">incoming</field>
            <field name="name">Receipts</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="asset_transfer_type_out_wl" model="asset.transfer.type">
            <field name="sequence_code">OUT</field>
            <field name="code">outgoing</field>
            <field name="name">Delivery</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="asset_transfer_type_int_wl" model="asset.transfer.type">
            <field name="sequence_code">INT</field>
            <field name="code">internal</field>
            <field name="name">Internal Transfer</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="machine_transfer_seq" model="ir.sequence">
            <field name="name">Equipment Transfer Sequence</field>
            <field name="code">account.machine.transfer</field>
            <field name="prefix">TRANSFER/</field>
            <field name="padding">5</field>
            <field name="active">TRUE</field>
            <field name="padding">0</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="sequence_product_rr" model="ir.sequence">
            <field name="name">Reordering Rule Sequence</field>
            <field name="code">reordering.rule</field>
            <field name="prefix">RR/</field>
            <field name="padding">5</field>
            <field name="active">TRUE</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="mail_act_reordering_reminder" model="mail.activity.type">
            <field name="name">Reordering Reminder</field>
            <field name="icon">fa-retweet</field>
            <field name="res_model_id" ref="account_asset_management.model_reordering_rule"/>
        </record>
        <!-- This Sequence is used to Generate the Equipment code -->
        <record id="seq_account_asset" model="ir.sequence">
            <field name="name">Equipment Code</field>
            <field name="code">account.asset</field>
            <field name="prefix"/>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>



    </data>
</odoo>